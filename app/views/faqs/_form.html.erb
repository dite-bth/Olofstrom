<%= form_for(@faq) do |f| %>
  <% if @faq.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@faq.errors.count, "error") %> prohibited this faq from being saved:</h2>

      <ul>
      <% @faq.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="row">
<div class="small-12 large-6 columns">
  <div class="field">
    <label for="faq_frequent_questions">Fråga:</label>
    <%= f.text_field :frequent_questions %>
  </div>
</div>
</div>
<div class="row">
<div class="small-12 large-6 columns">
  <div class="field">
    <label for="faq_frequent_answers">Svar:</label>
    <%= f.text_area :frequent_answers, :class => "tinymce" %>
  </div>
</div>
</div>
  <div class="actions">
    <%= f.submit 'Lägg till', :class => 'button form-btn tiny' %>
  </div>
<% end %>
<%= tinymce %>
